{
  "name": "vue-adminlte",
  "version": "1.0.0",
  "description": "a vue.js project for adminlte starter",
  "author": "andre-ly <liuyong1352@126.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "unit": "jest test/unit/specs --coverage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "build": "node build/build.js"
  },
  "dependencies": {
    "@liveqing/liveplayer": "^2.3.1",
    "admin-lte": "^2.4.8",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^3.3.7",
    "bootstrap-table": "^1.16.0",
    "echarts": "^4.9.0",
    "element-ui": "^2.13.0",
    "font-awesome": "^4.7.0",
    "jquery": "^2.2.3",
    "js-cookie": "^2.2.1",
    "select2": "^4.0.13",
    "stylus": "^0.54.8",
    "stylus-loader": "^5.0.0",
    "vue": "^2.5.2",
    "vue-baidu-map": "^0.21.22",
    "vue-layer": "^1.1.8",
    "vue-router": "^3.0.1",
    "vuex": "^3.1.3",
    "wangeditor": "^3.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^2.0.1",
